{
    "ComputeServers": {
        "AWS": {
            "FaaSType": "Lambda",
            "Region": "us-east-1",
            "UseSecretStore": true,
            "CPUsPerTask": 1,
            "Memory": 512,
            "TimeLimit": 900
        }
    },
    "DataStores": {
        "My_S3_Bucket": {
            "Endpoint": "https://play.min.io",
            "Bucket": "faasr",
            "Region": "us-east-1"
        }
    },
    "ActionList": {
        "rfunc": {

            "FunctionName": "r_log",
            "FaaSServer": "AWS",
            "Type": "R",
            "Arguments": {
                "message": "testing testing testing"
            },
            "InvokeNext": ["rfunc2"]
        },
        "rfunc2": {
            "FunctionName": "r_log",
            "FaaSServer": "AWS",
            "Type": "R",
            "Arguments": {
                "message": "testing testing testing"
            },
            "InvokeNext": []
        }
    },
    "ActionContainers": {
        "rfunc": "faasr/aws-lambda-r:dev",
        "rfunc2": "ramrakha/slurm-r:dev"
    },
    "FunctionCRANPackage": {
        "r_log": [
            "gapminder"
        ]
    },
    "FunctionGitRepo": {
        "r_log": "nolcut/test-py"
    },
    "FunctionInvoke": "rfunc",
    "FaaSrLog": "FaaSrLog",
    "LoggingDataStore": "My_S3_Bucket",
    "DefaultDataStore": "My_S3_Bucket",
    "InvocationID": "1234",
    "InvocationIDFromDate": "%Y%m%d%H%M",
    "WorkflowName": "test-lambda"
}
