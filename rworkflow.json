{
    "ComputeServers": {
        "GCP": {
            "FaaSType": "GoogleCloud",
            "Namespace": "faasr-dev",
            "Region": "us-central1",
            "Endpoint": "https://run.googleapis.com/v2/projects/",
            "UseSecretStore": true,
            "ClientEmail": "faasr-workflow-runner@faasr-dev.iam.gserviceaccount.com",
            "TokenUri": "https://oauth2.googleapis.com/token"
        }
    },
    "DataStores": {
        "My_S3_Bucket": {
            "Bucket": "faasr-test-0001",
            "Region": "us-east-1",
            "Writable": "TRUE",
            "AccessKey": "My_S3_Bucket_ACCESS_KEY",
            "SecretKey": "My_S3_Bucket_SECRET_KEY"
        }
    },
    "ActionList": {
        "rfunc": {
            "FunctionName": "r_log",
            "FaaSServer": "GCP",
            "Type": "R",
            "Arguments": {
                "message": "testing testing testing"
            },
            "InvokeNext": ["rfunc2"]
        },
         "rfunc2": {
            "FunctionName": "r_log",
            "FaaSServer": "GCP",
            "Type": "R",
            "Arguments": {
                "message": "testing testing testing"
            },
            "InvokeNext": []
        }
    },
    "ActionContainers": {
        "rfunc": "ramrakha/gcp-r:1.4",
        "rfunc2": "ramrakha/gcp-r:1.4"
    },
    "FunctionCRANPackage": {
        "r_log": [
            "gapminder"
        ]
    },
    "FunctionGitRepo": {
        "r_log": "nolcut/test-py"
    },
    "FunctionInvoke": "rfunc",
    "FaaSrLog": "FaaSrLog",
    "LoggingDataStore": "My_S3_Bucket",
    "DefaultDataStore": "My_S3_Bucket",
    "InvocationID":"1234",
    "InvocationIDFromDate": "%Y%m%d%H%M",
    "WorkflowName": "test-gcp"
}
